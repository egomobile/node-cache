{
  "name": "@egomobile/cache",
  "version": "0.1.0",
  "description": "A memory cache for Node.js",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16.0.0 < 17",
    "npm": ">=8.0.0 < 9"
  },
  "scripts": {
    "build": "del ./lib && tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --ignore 'src/test/**/*.ts' --exec node -r ts-node/register --inspect=0.0.0.0:9229 src/index.ts",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts --inspect=0.0.0.0:9229",
    "start": "node lib/index.js"
  },
  "keywords": [
    "node",
    "js",
    "ts",
    "javascript",
    "typescript",
    "cache",
    "memory"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/node-cache"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "dependencies": {
    "@egomobile/http-server": "0.60.3",
    "@types/node": "16.18.10"
  },
  "devDependencies": {
    "@egomobile/tsconfig": "^5.0.0",
    "del-cli": "5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-ego": "^0.17.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  }
}
